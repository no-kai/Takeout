<div class="container">
  <div class="row">
    <div class="col-md-9">
      投稿一覧
      <%= render "index", posts: @posts %>
    </div>
    <div class="col-md-2 offset-md-1">
      <%= link_to '新規投稿をする',new_post_path, class: "btn btn-info btn-sm btn-block" %>
      <br><br>
      <p>👑ランキング👑</p>
      <p><% @all_ranks.each do |post| %></p>
      <%= post.title %>
      <% end %>

    </div>
  </div>
</div>